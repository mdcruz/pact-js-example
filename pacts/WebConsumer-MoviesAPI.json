{
  "consumer": {
    "name": "WebConsumer"
  },
  "interactions": [
    {
      "description": "a request for all movies",
      "request": {
        "method": "GET",
        "path": "/movies"
      },
      "response": {
        "body": [
          {
            "id": 1,
            "name": "My movie",
            "year": 1999
          }
        ],
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            }
          },
          "header": {},
          "status": {}
        },
        "status": 200
      }
    },
    {
      "description": "a request to a specific movie",
      "providerStates": [
        {
          "name": "Has a movie with specific ID",
          "params": {
            "id": 100
          }
        }
      ],
      "request": {
        "method": "GET",
        "path": "/movie/100"
      },
      "response": {
        "body": {
          "id": 100,
          "name": "My movie",
          "year": 1999
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "integer"
                }
              ]
            },
            "$.name": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.year": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "integer"
                }
              ]
            }
          },
          "status": {}
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "12.5.2"
    },
    "pactRust": {
      "ffi": "0.4.20",
      "models": "1.2.0"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "MoviesAPI"
  }
}