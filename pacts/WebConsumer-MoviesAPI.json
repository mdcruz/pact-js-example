{
  "consumer": {
    "name": "WebConsumer"
  },
  "interactions": [
    {
      "description": "a request for all movies",
      "request": {
        "method": "GET",
        "path": "/movies"
      },
      "response": {
        "body": [
          {
            "id": 1,
            "name": "My movie",
            "year": 1999
          }
        ],
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            }
          },
          "header": {}
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "12.1.0"
    },
    "pactRust": {
      "ffi": "0.4.9",
      "models": "1.1.11"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "MoviesAPI"
  }
}